<h1 class="header center gray-text">Courses</h1>
<div class="container">
<div class="card">
<table>
  <thead>
    <tr>
      <th>Clave</th>
      <th>Nombre de la Materia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.course %></td>
        <td><%= course.c_name %></td>
        <td><%= link_to 'Editar', edit_course_path(course) %></td>
        <td><%= link_to 'Eliminar', course, method: :delete, data: { confirm: 'EstÃ¡ completamente seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Curso', new_course_path %>
</div>
</div>
