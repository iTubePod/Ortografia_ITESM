  <br><br>
  <div class="container">
    <div class="container">
      <div class="container">
        <div class="card">
          <div class="container">
          <div class="icon-block">
            <br>
            <h2 class="center blue-text"><i class="medium material-icons">bookmark</i></h2>
            <h5 class="center">Entrega de estudiantes</h5>
            <li class="divider"></li>
            <br>
              <%- @question_group_results.each do |result| %>
                <div>
                  <h4><%= result.question.question_text %></h4>
                  <p>
                    <%- if result.results.is_a?(Array) %>
                      <ol>
                        <%- result.results.each do |answer| %>
                          <li><%= answer %></li>
                        <% end %>
                      </ol>
                    <% elsif result.results.is_a?(Hash) %>
                      <table>
                        <thead>
                          <tr>
                            <th>Option</th>
                            <th>Count</th>
                          </tr>
                        </thead>
                        <tbody>
                          <%- result.results.each do |option, count| %>
                            <tr>
                              <td><%= option %></td>
                              <td><%= count %></td>
                            </tr>
                          <% end %>
                        </tbody>
                      </table>
                    <% end %>
                </div>
                <li class="divider"></li>
              <% end %>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
