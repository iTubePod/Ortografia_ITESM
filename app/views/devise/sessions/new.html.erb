  <div class="navbar-fixed">
    <nav class="blue lighten-1" role="navigation">
        <div class="nav-wrapper">
          <a id="logo-container" class="brand-logo"><img style="border: none;" src="/icon.png"></a>
        </div>
    </nav>
  </div>
  <br><br>
  <br>
  <div class="container">
    <div class="container">
      <div class="container">
        <div class="card">
          <div class="container">
            <br />
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <div class="row">
            <div class="input-field">
            <img class="responsive-img" src="/logo.png">
          </div>
          </div>

          <div class="row">
            <%= content_tag(:div, flash[:error], :id => "flash_error") if flash[:error] %>
            <%= content_tag(:div, flash[:notice], :id => "flash_notice") if flash[:notice] %>
            <%= content_tag(:div, flash[:alert], :id => "flash_alert") if flash[:alert] %>
            <div class="input-field">
              <p class="teal-text text-lighten-2">Username</p>
              <%= f.text_field :matricula%>
            </div>
          </div>

          <div class="row">
            <div class="input-field">
              <p class="teal-text text-lighten-2">Password</p>
              <%= f.password_field :password, autocomplete: "off" %>
            </div>
          </div>

          <div class="row">
            <% if devise_mapping.rememberable? -%>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
            <% end -%>
            </div>
            <div class="row">
              <button class="btn waves-effect waves-light" type="submit" name="action">Iniciar Sesi√≥n
                <i class="material-icons right">send</i>
              </button>
            </div>
            <br />
            <% end %>
            <%= render "devise/shared/links" %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <br><br>
  <br>